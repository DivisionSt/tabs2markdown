{
  "manifest_version": 3,
  "name": "Tabs2Markdown",
  "version": "1.0",
  "description": "Convert Chrome tabs to markdown format with search and selection features",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup_html.html",
    "default_title": "Tabs2Markdown"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
